<GridPane alignment="center" hgap="30" prefHeight="432.0" prefWidth="421.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.WelcomeController">
    <padding>
        <Insets bottom="10" left="25" right="25" top="25" />
    </padding>
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="30.0" />
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="30.0" />
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="30.0" />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <Button alignment="CENTER" onAction="#handleLoginPressed" prefHeight="27.0" prefWidth="70.0" GridPane.columnIndex="1" GridPane.rowIndex="1">Login
    </Button>
    <Button alignment="CENTER" onAction="#handleRegisterPressed" prefHeight="2.0" prefWidth="71.0" GridPane.columnIndex="1" GridPane.rowIndex="2">Register
    </Button>
   <ImageView fitHeight="150.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1">
      <image>
         <Image url="@bobertWaters.png" />
      </image>
   </ImageView>
   <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="2">
      <image>
         <Image url="@robert_waters_jr.jpg" />
      </image>
   </ImageView>
   <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="2">
      <image>
         <Image url="@robert_waters_jr.jpg" />
      </image>
   </ImageView>
</GridPane>
